class Datatypes{

public static void main(String args[])
{
    String str = 'khushid';
    System.out.println("String is:"+str);

    String str1 = new String ('khushid');
    System.out.println("Another String is:"+str1);

    int arr[];
    arr = neww int[2];
    arr[0] = 0;
    arr[1]= 1;
    //arr[2]}=2;

    System.out.println(arr);
    System.out.println(arr[0]);
}
}